# This file tests if input/output redirect expressions properly insert a
# (hidden) sequence node it they get more than one argument.

(header (u32.const 0x6d736163) (u32.const 0x0))
(void)

(define 'code' (params)
   (read)
   (peek (uint8))
   (read (uint8))
   (write (uint8) (i32.const 8))
   (peek (uint8) (uint8))
   (read (uint8) (uint8)))

